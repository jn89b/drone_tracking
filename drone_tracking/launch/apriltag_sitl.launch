<launch>

    <!--args for apritag_ros-->
    <arg name="launch_prefix" default="" /> <!-- set to value="gdbserver localhost:10000" for remote debugging -->
    <arg name="node_namespace" default="apriltag_ros_continuous_node" />
    <arg name="camera_name" default="/iris/usb_cam" />
    <!-- <arg name="camera_frame" default="/robot_camera_link" /> -->
    <arg name="image_topic" default="image_rect" />
    
    <!--Run image proc-->
    <node 
    pkg="image_proc" 
    type="image_proc" 
    name="image_proc" 
    ns="/iris/usb_cam">
    </node>

    <!--Run apriltag ros-->
    <include file="$(find apriltag_ros)/launch/continuous_detection.launch">
        <arg name="launch_prefix" value="$(arg launch_prefix)" /> <!-- set to value="gdbserver localhost:10000" for remote debugging -->
        <arg name="node_namespace" value="$(arg node_namespace)" />
        <arg name="camera_name" value="$(arg camera_name)" />
        <!-- <arg name="camera_frame" value="$(arg camera_frame)" /> -->
        <arg name="image_topic" value="$(arg image_topic)" />
	</include>

    <!-- ekf apriltag estimation
    <node 
    pkg="drone_tracking"
    type="ekf_tag.py"
    name="ekf_tag"/> -->
    
</launch>       